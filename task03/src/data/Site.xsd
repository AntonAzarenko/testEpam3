<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="www.azarenko.com" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="site" type="azar:siteType" xmlns:azar="www.azarenko.com"/>
  <xs:complexType name="typeType">
    <xs:sequence>
      <xs:element type="azar:advertisingpageType" name="advertisingpage" minOccurs="0" xmlns:azar="www.azarenko.com"/>
      <xs:element type="azar:newpageType" name="newpage" minOccurs="0" xmlns:azar="www.azarenko.com"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="pageType">
    <xs:sequence>
      <xs:element name="title">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="advertisingpage"/>
            <xs:enumeration value="news"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="azar:typeType" name="type" xmlns:azar="www.azarenko.com"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="newpageType">
    <xs:sequence>
      <xs:element type="xs:string" name="email"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="advertisingpageType">
    <xs:sequence>
      <xs:element type="xs:string" name="email"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="siteType">
    <xs:sequence>
      <xs:element type="azar:pageType" name="page" maxOccurs="unbounded" minOccurs="0" xmlns:azar="www.azarenko.com"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>